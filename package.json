{
  "name": "rbac-system",
  "license": "UNLICENSED",
  "packageManager": "yarn@4.6.0",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "yarn docker:up && yarn docker:watch",
    "docker:up": "docker-compose -p rbac-system-backend up -d --build --force-recreate",
    "docker:watch": "docker-compose -p rbac-system-backend logs -f --tail 10 --no-log-prefix",
    "docker:stop": "docker-compose -p rbac-system-backend stop",
    "docker:bash": "docker exec -it rbac-system-backend bash",
    "build": "yarn tsc && yarn tsc-alias",
    "start:dev": "nodemon --watch . --exec 'eslint --fix && tsx -r tsconfig-paths/register src/main.ts'",
    "start": "node dist/main.js",
    "seed": "tsx seeders/runSeeders.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@types/bcrypt": "^5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9",
    "@types/module-alias": "^2",
    "@types/node": "^22.10.5",
    "@types/supertest": "^6.0.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.5.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.21",
    "module-alias": "^2.2.3",
    "mongoose": "^8.9.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "zod": "^3.24.1",
    "zod-express-middleware": "^1.4.0"
  }
}